@model IEnumerable<GameStore.Models.AspNetUsers>

@{
    ViewData["Title"] = "Index";
}
<style>
    h1 {
        margin-bottom: 0.5em;
    }

    .adminToggle {
        margin-top: 1em;
    }
</style>


<div>
    <div>
        <h1>Admin Panel</h1>
    </div>
    <div class="buttonContainer">
        <a class="btnGame btn btn-success" asp-area="" asp-controller="Game" asp-action="Index">Games</a>
        <a class="btnEvent btn btn-secondary" asp-area="" asp-controller="Event" asp-action="Index">Events</a>
        <a asp-action="Review" class="btnReview btn btn-info">Review</a>
        <a class="btnReport btn btn-warning">Reports</a>
    </div>
</div>




@section Scripts
    {
    <script type="text/javascript">
        $(document).ready(function () {
        });
    </script>
}
