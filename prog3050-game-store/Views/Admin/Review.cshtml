@model IEnumerable<GameStore.Models.Review>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{

}
@foreach (var item in Model)
{
    <div class="card" style="width: 18rem;">
        <div class="card-body">
            <h3 class="card-title">User: ${data.userName}</h3>
            <p card-text>Title ${data.title}</p>
            <p card-text>Description${data.review1}</p>
            <button id="btnApproved" type="button" class="btn btn-primary">Approve</button>
            <button value="${data.reviewId}" id="btnDeclined" type="button" class="btn btn-danger">Decline</button>
        </div>
    </div>
}

